---
import { projects } from "../../features/project/projects";
import type { GetStaticPaths } from "astro";

export const getStaticPaths = (() => {
  const paths = projects.map((project) => ({
    params: { slug: project.href.replace("/projects/", "") },
  }));
  return paths;
}) satisfies GetStaticPaths;

const { slug } = Astro.params;

const project = projects.find(
  (project) => project.href === `/projects/${slug}`
);
import Layout from "../../layouts/Layout.astro";
import MainContainer from "../../components/MainContainer.astro";
import Maintenance from "../../features/project/Maintenance";
---

<Layout>
  <MainContainer> asd </MainContainer>
</Layout>
